<script>
$("table.chart-bar").each(function() {
    var colors = [];
    $("table.chart-bar thead th:not(:first)").each(function() {
        colors.push($(this).css("color"));
    });
    
    var flotOptions = {
		bars: {
	        align:  "center",
	        horizontal: false
	    },
	    yaxis: {
	    	min:0,
	    	max:100,
	    }
    };
    $(this).graphTable({
        series: 'columns',
        position: 'replace',
        width: 977,
        height: 500,
        colors: colors,
    }, flotOptions);
});



</script>