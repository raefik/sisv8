<h1>Listing commentar</h1>

<table>
  <tr>
    <th>User_id</th>
    <th>Student_id</th>
    <th>Commentar</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @commentars.each do |commentar| %>
  <tr>
    <td><%= commentar.user_id %></td>
    <td><%= commentar.student_id %></td>
    <td><%= commentar.remark %></td>
    <td><%= link_to 'Show', commentar %></td>
    <td><%= link_to 'Edit', edit_comment_path(commentar) %></td>
    <td><%= link_to 'Destroy', commentar, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

